@using ImportacionExcel.Models;
@model List<FiltroB>

<table class="table table-striped table-bordered">
    <caption>INFORMACIÓN FILTRADA</caption>
    <thead>
        <tr>
            <th class="text-center">Usuario</th>
            <th class="text-center">Cédula</th>
            <th class="text-center">Correo</th>
        </tr>
    </thead>
    <tbody>
        @foreach (FiltroB c in Model)
        {
            <tr>
                <td class="text-center">@c.Usuario</td>
                <td class="text-center">@c.Cedula</td>
                <td class="text-center">@c.Correo</td>
            </tr>
        }
    </tbody>
</table>