@using ImportacionExcel.Models;
@model IEnumerable<FiltroD>

<table class="table table-striped table-bordered">
    <caption>INFORMACIÓN FILTRADA</caption>
    <thead>
        <tr>
            <th class="text-center">Usuario</th>
            <th class="text-center">Edad</th>
            <th class="text-center">Sexo</th>
        </tr>
    </thead>
    <tbody>
        @foreach (FiltroD c in Model)
        {
            <tr>
                <td class="text-center">@c.Usuario</td>
                <td class="text-center">@c.Edad</td>
                <td class="text-center">@c.Sexo</td>
            </tr>
        }
    </tbody>
</table>